@{
  Layout = "_Layout";
}

@model Factory.Models.Engineer

<div class="mainBox">
  <h2>Are you sure you want to delete Engineer <strong>@Html.DisplayFor(model => model.EngineerName)</strong>?</h2>
  <hr />  
  @using (Html.BeginForm())
  {
      <input type="submit" value="Delete!" class="delete" />
  }
  @Html.ActionLink("Back to Engineers", "Index",null,null, new { @class = "btn btn-info btn-large" })
</div>